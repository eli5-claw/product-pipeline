# Example environment variables
# Copy this file to .env and fill in your credentials

# Polymarket API credentials
BOT_POLYMARKET__API_KEY=your_api_key_here
BOT_POLYMARKET__API_SECRET=your_api_secret_here
BOT_POLYMARKET__PRIVATE_KEY=your_private_key_here

# Trading settings
BOT_TRADING__ENABLED=true
BOT_TRADING__MIN_EDGE=0.02

# Logging
RUST_LOG=info
RUN_MODE=development
